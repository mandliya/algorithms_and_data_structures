#include<bits/stdc++.h>
using namespace std;
int main() {

	// Write your code here
    long n;
    cin>>n;
    long * arr=new long[n];
    for(int i=0;i<n;i++)
        cin>>arr[i];
    sort(arr,arr+n,greater<long>());
  //  for(int i=0;i<n;i++)
   //     cout<<arr[i]<<" ";
    int f=0;
    for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            for(int k=j+1;k<n;k++)
            {
                if(arr[i]<(arr[j]+arr[k]))
                {
                    cout<<arr[k]<<" "<<arr[j]<<" "<<arr[i]<<endl;
                     f=1;
                  
                   j=n;
                    i=n;
                       break;
                   
                }
            }
        }
    }
    if(f==0)
        cout<<-1;
}
