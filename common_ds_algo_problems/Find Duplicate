#include <iostream>
using namespace std;
int duplicateNumber(int *arr, int size)
{
    int i;
    int newsum=((size-2)*(size-1))/2;   //n(n+1)/2 sum of n natural no.
    int sum=0;
    for(i=0;i<size;i++){
    sum=sum+arr[i];
    }
    return sum-newsum;
} 


int main()
{

	int t;
	cin >> t;
	
	while (t--)
	{
		int size;
		cin >> size;
		int *input = new int[size];

		for (int i = 0; i < size; i++)
		{
			cin >> input[i];
		}

		cout << duplicateNumber(input, size) << endl;
	}

	return 0;
}
